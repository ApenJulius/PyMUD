sudo aa-remove-unknown

docker build -t tt-client .
docker run -p 8080:8080 tt-client

cp /usr/games/tt++ tt++
wget https://github.com/tsl0922/ttyd/releases/download/1.7.7/ttyd.x86_64


sudo ufw allow proto tcp from 172.17.0.0/16 to 127.0.0.1 port 4001
sudo ufw deny out from 172.17.0.0/16
sudo ufw allow out from 172.17.0.0/16 to 0.0.0.0 port 8080




